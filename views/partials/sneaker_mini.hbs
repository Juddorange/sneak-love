<a href="/one-product/{{this._id}}" class="product-item-wrapper">
    <div class="product-img">
        <img src="{{this.picPath}}" alt="{{this.picName}} : what a nice pair of kicks">
    </div>
    <p class="product-name">{{this.name}}</p>
    <p class="product-cat">{{this.category}}</p>
    <p class="product-price">{{this.price}}</p>
    <p class="product-tag" data-id-tag="{{this.id_tags}}">{{this.label}}</p>
</a>